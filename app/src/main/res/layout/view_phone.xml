<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2c6173"
    android:orientation="vertical">


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/di"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="top"
            android:layout_marginTop="120dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/b1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#FFFFFF"
                android:textSize="30dp"
                android:paddingRight="15dp"
                android:paddingLeft="15dp"
                android:gravity="center"
                android:text="666666611188888888888000008888888888888888888886"
                android:singleLine="true"
                />
            <TextView
                android:id="@+id/b2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#FFFFFF"
                android:textSize="20dp"
                android:paddingRight="15dp"
                android:paddingLeft="15dp"
                android:layout_marginTop="15dp"
                android:gravity="center"
                android:text="66888888888886"
                android:singleLine="true"
                />

        </LinearLayout>



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginTop="50dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_gravity="center"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <LinearLayout

                    android:id="@+id/a1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/icon1"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/jujie0"/>

                    <TextView
                        android:id="@+id/ti1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#BAFFFFFF"
                        android:textSize="14dp"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="添加通话"
                        />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/a2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/icon2"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/jujie0"/>

                    <TextView
                        android:id="@+id/ti2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#BAFFFFFF"
                        android:textSize="14dp"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="通话音量"
                        />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/a3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/icon3"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/jujie0"/>

                    <TextView
                        android:id="@+id/ti3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#BAFFFFFF"
                        android:textSize="14dp"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="蓝牙"
                        />

                </LinearLayout>






            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_marginTop="20dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/a4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/icon4"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/jujie0"/>

                    <TextView
                        android:id="@+id/ti4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#BAFFFFFF"
                        android:textSize="14dp"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="扬声器"
                        />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/a5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/icon5"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/jujie0"/>

                    <TextView
                        android:id="@+id/ti5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#BAFFFFFF"
                        android:textSize="14dp"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="静音"
                        />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/a6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/icon6"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/jujie0"/>

                    <TextView
                        android:id="@+id/ti6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#BAFFFFFF"
                        android:textSize="14dp"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="拨号盘"
                        />

                </LinearLayout>
            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginBottom="70dp"
            android:layout_gravity="bottom"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/jtgd"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:scaleType="fitXY"
                android:src="@drawable/jujie0"/>
        </LinearLayout>

    </FrameLayout>

</LinearLayout>